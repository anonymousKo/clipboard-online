<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ke.clipboard.dao.CopyTextDao">

    <select id="find" resultType="com.ke.clipboard.model.CopyText">
        select * from message
    </select>
    <select id="insert" resultType="com.ke.clipboard.model.CopyText">
        insert into message (msg,addTime) VALUES (#{msg},#{addTime})
    </select>
    <select id="deletePreviousData" resultType="com.ke.clipboard.model.CopyText">
        delete from message where addTime &lt; #{deleteDate};
    </select>
</mapper>
